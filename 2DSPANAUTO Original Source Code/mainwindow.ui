   <string notr="true">/* Main Application Styling */
QMainWindow {
    background-color: #2D2D30;
}

QWidget {
    font-family: 'Segoe UI', Arial, sans-serif;
    font-size: 12px;
    color: #DCDCDC;
}

/* Tab Widget Styling */
QTabWidget::pane {
    border: 1px solid #3F3F46;
    border-radius: 4px;
    margin: 2px;
}

QTabBar::tab {
    background: #404040;
    color: #B0B0B0;
    padding: 8px 15px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border: 1px solid #3F3F46;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background: #007ACC;
    color: white;
    border-color: #005FAB;
}

QTabBar::tab:hover {
    background: #505050;
}

/* Button Styling */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #505050, stop:1 #404040);
    border: 1px solid #3F3F46;
    border-radius: 4px;
    color: #E0E0E0;
    padding: 5px 10px;
    min-width: 80px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #606060, stop:1 #505050);
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #404040, stop:1 #303030);
}

/* Group Box Styling */
QGroupBox {
    border: 1px solid #3F3F46;
    border-radius: 4px;
    margin-top: 10px;
    padding-top: 15px;
    color: #B0B0B0;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    left: 10px;
}

/* Spin Box &amp; Slider Styling */
QSpinBox, QDoubleSpinBox {
    background: #404040;
    border: 1px solid #3F3F46;
    border-radius: 3px;
    padding: 3px;
    color: #E0E0E0;
}

QSlider::groove:horizontal {
    height: 6px;
    background: #404040;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #007ACC;
    width: 14px;
    margin: -4px 0;
    border-radius: 7px;
}

QSlider::sub-page:horizontal {
    background: #007ACC;
    border-radius: 3px;
}

/* List Widget Styling */
QListWidget {
    background: #404040;
    border: 1px solid #3F3F46;
    border-radius: 4px;
    color: #E0E0E0;
}

QListWidget::item:hover {
    background: #505050;
}

QListWidget::item:selected {
    background: #007ACC;
    color: white;
}

/* Progress Bar Styling */
QProgressBar {
    border: 1px solid #3F3F46;
    border-radius: 3px;
    text-align: center;
    background: #404040;
}

QProgressBar::chunk {
    background: #00CC00;
    border-radius: 2px;
}

/* Tool Box Styling */
QToolBox::tab {
    background: #404040;
    color: #B0B0B0;
    border: 1px solid #3F3F46;
    border-radius: 4px;
    padding: 5px;
    margin: 2px;
}

QToolBox::tab:selected {
    background: #007ACC;
    color: white;
}

/* Line Edit Styling */
QLineEdit {
    background: #404040;
    border: 1px solid #3F3F46;
    border-radius: 3px;
    padding: 5px;
    color: #E0E0E0;
}

/* Scroll Bar Styling */
QScrollBar:vertical {
    width: 12px;
    background: #404040;
}

QScrollBar::handle:vertical {
    background: #505050;
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
}

/* Status Bar Styling */
QStatusBar {
    background: #252526;
    color: #B0B0B0;
}

/* Menu Bar Styling */
QMenuBar {
    background: #333337;
    color: #B0B0B0;
}

QMenuBar::item:selected {
    background: #505050;
    color: white;
}

QMenu {
    background: #333337;
    border: 1px solid #3F3F46;
    color: #B0B0B0;
}

QMenu::item:selected {
    background: #007ACC;
    color: white;
}</string>
